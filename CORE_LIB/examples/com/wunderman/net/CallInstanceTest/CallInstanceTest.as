package com.balt.net{	import com.balt.events.GenericDataEvent;	import flash.events.IOErrorEvent;		public class CallInstanceTest	{		private var myCallInstance:CallInstance;				public function CallInstanceTest():void {			// unit test of CallInstance class			myCallInstance = new CallInstance();			myCallInstance.addEventListener(CallInstance.DATA_LOADED, receiveData);			myCallInstance.addEventListener(IOErrorEvent.IO_ERROR, dataError);			myCallInstance.sendData("test.xml");		}						private function receiveData(evt:GenericDataEvent):void		{			trace ("Data received is " + evt.data);		}				private function dataError(evt:IOErrorEvent):void		{			trace ("Data error", evt.text);		}			}}